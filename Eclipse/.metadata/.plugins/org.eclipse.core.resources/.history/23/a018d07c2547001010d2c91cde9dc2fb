package br.com.senaisp.bauru.classes;

public class Estudante {
	//fields ou campos ou variaveis de instancia 
	private String nome;
	private int ra;
	private double nota;
	private boolean ehfumante;
	//constructor
	public Estudante() {
		nome = "Sem nome;"
		ra = 0;
		nota = 0.0;
		ehfumante = false;
		
	}
}
